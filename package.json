{
  "name": "vite-plugin-rsw",
  "description": "wasm-pack plugin for Vite",
  "version": "1.9.1",
  "author": "lencx <cxin1314@gmail.com>",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
